{
    "name": "pxp/pxp",
    "description": "The PXP Programming Language",
    "keywords": ["pxp", "php", "transpiler", "superset"],
    "homepage": "https://pxplang.org",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/pxp-lang/pxp/issues",
        "source": "https://github.com/pxp-lang/pxp"
    },
    "authors": [
        {
            "name": "Ryan Chandler",
            "email": "support@ryangjchandler.co.uk"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel-zero/framework": "^10.0.2",
        "nunomaduro/termwind": "^1.15.1",
        "phpactor/language-server": "^5.1",
        "pxp/parser": "v0.0.4"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.8",
        "mockery/mockery": "^1.5.1",
        "pestphp/pest": "^2.5",
        "phpstan/phpstan": "^1.10",
        "spatie/laravel-ray": "^1.32"
    },
    "autoload": {
        "psr-4": {
            "Pxp\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pxp\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "phpstan": {
            "includes": [
                "./app/PhpStan/extension.neon"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["pxp"]
}
